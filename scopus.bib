@BOOK{DeLaRosaAlgarín2015487,
author={De La Rosa Algarín, A. and Demurjian, S.A. and Ziminski, T.B. and Rivera Sánchez, Y.K. and Kuykendall, R.},
title={Securing XML with role-based access control: Case study in health care},
