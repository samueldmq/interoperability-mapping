@inproceedings{ ISI:000380402000043,
Author = {Schleicher, Johannes M. and Voegler, Michael and Inzinger, Christian and
   Dustdar, Schahram},
Editor = {{Altintas, O and Zhang, J}},
Title = {{Smart Fabric - An Infrastructure-Agnostic Artifact Topology Deployment
