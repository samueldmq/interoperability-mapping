@inproceedings{ ISI:000380438000053,
Author = {Lilis, Georgios and Conus, Gilbert and Asadi, Nastaran and Kayal, Maher},
Book-Group-Author = {{IEEE}},
Title = {{Integrating Building Automation Technologies with Smart Cities An
   Assessment Study of past, Current and Future Interroperable Technologies}},
Booktitle = {{SMARTGREENS 2015 PROCEEDINGS OF THE 4TH INTERNATIONAL CONFERENCE ON
   SMART CITIES AND GREEN ICT SYSTEMS}},
Year = {{2015}},
Pages = {{370-375}},
Note = {{4th International Conference on Smart Cities and Green ICT Systems / 1st
   International Conference on Vehicle Technology and Intelligent Transport
   Systems (VEHITS), Lisbon, PORTUGAL, MAY 20-22, 2015}},
Organization = {{Inst Syst \& Technol Informat Control \& Communicat; IEEE Power \&
   Energy Soc; Assoc Comp Machinery SIGCAS; IEEE Tech Area Green Comp; IEEE
   Technical Sub Comm Green Communicat \& Comp; Friends Supergrid; Network
   Sustainable Ultrascale Comp; Lisbon City Council; INTELI; Lisboa E Nova;
   Turismo Lisboa; CEIIA}},
Abstract = {{Future smart cities would integrate a wide range of mostly heterogeneous
   systems and ICT is an essential asset in the coordination of those. The
   smart buildings, a major smart cities research and development domain,
   should advance beyond the complex automation tools and the anticipated
   energy and comfort envelope. The universal convergence to technologies
   that would enable the seamless integration with the anticipated smart
   cities urban environment should be highlighted. Although it is a concept
   widely accepted for current and future developing standards, it is much
   less communicated across scientific fields as for example the urban
