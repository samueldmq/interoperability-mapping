Title = {{Towards a Model-Driven Solution to the Vendor Lock-in Problem in Cloud
   Computing}},
Booktitle = {{2013 IEEE FIFTH INTERNATIONAL CONFERENCE ON CLOUD COMPUTING TECHNOLOGY
   AND SCIENCE (CLOUDCOM), VOL 1}},
Series = {{International Conference on Cloud Computing Technology and Science}},
Year = {{2013}},
