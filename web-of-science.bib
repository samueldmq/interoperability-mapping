@article{ ISI:000371811300001,
Author = {Kapoor, Lohit and Bawa, Seema and Gupta, Ankur},
Title = {{Peer Clouds: A P2P-Based Resource Discovery Mechanism for the Intercloud}},
Journal = {{INTERNATIONAL JOURNAL OF NEXT-GENERATION COMPUTING}},
Year = {{2015}},
Volume = {{6}},
Number = {{3}},
Pages = {{153-164}},
Month = {{NOV}},
Abstract = {{The Intercloud represents the next logical step in the evolution of
   cloud computing overcoming issues of data/vendor lock-in and dealing
   with volatile service requests. However, resource discovery across
   heterogeneous Cloud Service Providers (CSPs) remains a challenge. In
   this paper we present a P2P-based distributed resource discovery
   mechanism based on spatial-awareness of cloud data-centers belonging to
   different Cloud Service Providers. The scheme is based upon exploiting
   location information of Data Centers and organizing them into DHT peers
   for optimal communication. It thus allows for QoS-compliant
   resource/service provisioning across Cloud Service Providers (CSPs).
   Simulation results establish the effectiveness of the proposed scheme.}},
