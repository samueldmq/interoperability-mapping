Title = {{Cloud Federation: Effects of Federated Compute Resources on Quality of
   Service and Cost}},
Booktitle = {{PROCEEDINGS OF THE 2013 IEEE INTERNATIONAL CONFERENCE ON CLOUD
   ENGINEERING (IC2E 2013)}},
Year = {{2013}},
Pages = {{31-37}},
