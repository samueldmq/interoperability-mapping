Title = {{Semantic Representation of Cloud Services: a Case Study for Microsoft
   Windows Azure}},
Booktitle = {{2014 INTERNATIONAL CONFERENCE ON INTELLIGENT NETWORKING AND
   COLLABORATIVE SYSTEMS (INCOS)}},
Year = {{2014}},
Pages = {{647-652}},
