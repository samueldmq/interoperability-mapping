@inproceedings{ ISI:000352079100066,
Author = {Silva, Gabriel Costa and Rose, Louis M. and Calinescu, Radu},
Book-Group-Author = {{IEEE}},
Title = {{A Systematic Review of Cloud Lock-in Solutions}},
Booktitle = {{2013 IEEE FIFTH INTERNATIONAL CONFERENCE ON CLOUD COMPUTING TECHNOLOGY
   AND SCIENCE (CLOUDCOM), VOL 2}},
Series = {{International Conference on Cloud Computing Technology and Science}},
Year = {{2013}},
Pages = {{363-368}},
Note = {{5th IEEE International Conference on Cloud Computing Technology and
   Science (IEEE CloudCom), Bristol, ENGLAND, DEC 02-05, 2013}},
Organization = {{IEEE; IEEE Comp Soc; IEEE STC Cloud Comp; IEEE Cloud Comp; Cloud Comp
   Assoc; Hewlett Packard Labs; Univ W England; IEEE CS TCSC}},
Abstract = {{The heterogeneity of cloud semantics, technology and interfaces limits
   application and platform portability and interoperability, and can
   easily lead to vendor lock-in. We identify, analyse and classify
   existing solutions to cloud vendor lock-in, and highlight unresolved
   challenges. Our survey is based on a systematic review of 721 primary
   studies that describe the state-of-the-art in managing cloud lock-in,
   portability and interoperability. 78 of these primary studies were
   selected and used for a thorough analysis of cloud standards, commercial
   products and academic work related to cloud lock-in. Our review shows
   that most solutions proposed so far are platforms, APIs or architectures
   addressing infrastructure-as-a-service (IaaS) interoperability. From our
   review, we identify a need for: (i) exploiting established solutions
   from areas that are closely related to cloud computing; (ii) increasing
   empirical evidence to raise confidence in existing solutions; and (iii)
   addressing the socio-technical and business challenges related to cloud
   lock-in.}},
