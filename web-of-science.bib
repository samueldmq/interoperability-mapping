Title = {{A Computing Resource Selection Approach Based on Genetic Algorithm for
   Inter-Cloud Workload Migration}},
Booktitle = {{MOVING INTEGRATED PRODUCT DEVELOPMENT TO SERVICE CLOUDS IN THE GLOBAL
   ECONOMY}},
Series = {{Advances in Transdisciplinary Engineering}},
Year = {{2014}},
