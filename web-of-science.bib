@inproceedings{ ISI:000396419000004,
Author = {Di Modica, Giuseppe and Tomarchio, Orazio},
Editor = {{Zimmerman, W and Lee, YW and Demchenko, Y}},
Title = {{A Semantic Model to Characterize Pricing and Negotiation Schemes of
   Cloud Resources}},
Booktitle = {{THIRD INTERNATIONAL CONFERENCE ON CLOUD COMPUTING, GRIDS, AND
   VIRTUALIZATION (CLOUD COMPUTING 2012)}},
Series = {{Cloud Computing-International Conference on Cloud Computing GRIDs and
   Virtualization}},
Year = {{2012}},
Pages = {{18-23}},
Note = {{3rd International Conference on Cloud Computing, GRIDs, and
   Virtualization (CLOUD COMPUTING), Nice, FRANCE, JUL 22-27, 2012}},
Organization = {{IARIA}},
Abstract = {{Cloud computing technology has reached a good level of maturity. The
   market of cloud resources is still dominated by proprietary solutions
   for what concerns resource delivering, pricing models and Service Level
   Agreement. The research community is working hard to define
   specifications that try to standardize most of these aspects. When
   standards will get mature, customers will no more be locked-up to any
   proprietary technology, and full interoperability among clouds will be a
   reality. In the future cloud resource market the competition challenge
   will be played on the real capability of providers to accommodate
   customers' requests in a flexible way and to supply high and
   differentiated QoS levels. In this scenario a mechanism must be devised
   to support the matchmaking between what providers offer and what
   customers' applications demand. In this work we propose the definition
   of a semantic model to support the supply-demand matchmaking process in
   the future cloud markets. Leveraging on a semantic description of the
   cloud resources' features, customers will be able to discover cloud
   offers that best suit their own business needs. Tests conducted on an
   implementation prototype proved the viability of the approach.}},
