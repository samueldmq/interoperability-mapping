@inproceedings{ ISI:000343582700002,
Author = {Rafique, Ansar and Walraven, Stefan and Lagaisse, Bert and Desair, Tom
   and Joosen, Wouter},
Book-Group-Author = {{IEEE}},
Title = {{Towards Portability and Interoperability Support in Middleware for
   Hybrid Clouds}},
