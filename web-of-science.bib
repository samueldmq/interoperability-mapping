@inproceedings{ ISI:000347383300002,
Author = {Habiba, Umme and Masood, Rahat and Shibli, Muhammad Awais},
Editor = {{Lee, R}},
Title = {{Secure Identity Management System for Federated Cloud Environment}},
Booktitle = {{SOFTWARE ENGINEERING, ARTIFICIAL INTELLIGENCE, NETWORKING AND
   PARALLEL/DISTRIBUTED COMPUTING}},
