@inproceedings{ ISI:000320473500062,
Author = {Hsieh, George and Chen, Rong-Jaye},
Book-Group-Author = {{IEEE}},
Title = {{Design for a Secure Interoperable Cloud-Based Personal Health Record
   Service}},
