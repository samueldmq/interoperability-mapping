@inproceedings{ ISI:000309952400001,
Author = {Marca, David and Bulander, Rebecca and Kruslin, Cornelia and Shishkov,
   Boris and van Sinderen, Marten},
Editor = {{Obaidat, MS and Tsihrintzis, GA and Filipe, J}},
Title = {{e-Business Challenges and Directions: Important Themes from the First
   ICE-B Workshop}},
Booktitle = {{E-BUSINESS AND TELECOMMUNICATIONS}},
Series = {{Communications in Computer and Information Science}},
Year = {{2012}},
Volume = {{222}},
Pages = {{3-35}},
Note = {{7th International Joint Conference on e-Business and Telecommunications
   (ICETE), Athens, GREECE, JUL 26-28, 2010}},
Abstract = {{A three-day asynchronous, interactive workshop was held at ICE-B'10 in
   Piraeus, Greece in July of 2010. This event captured conference themes
   for e-Business challenges and directions across four subject areas: a)
   e-Business applications and models. b) enterprise engineering, c)
   mobility, d) business collaboration and e-Services, and e) technology
   platforms. Quality Function Deployment (QFD) methods were used to
   gather, organize and evaluate themes and their ratings. This paper
   summarizes the most important themes rated by participants: a) Since
   technology is becoming more economic and social in nature, more agile
   and context-based application develop methods are needed. b) Enterprise
   engineering approaches are needed to support the design of systems that
   can evolve with changing stakeholder needs. c) The digital native
   groundswell requires changes to business models, operations, and systems
