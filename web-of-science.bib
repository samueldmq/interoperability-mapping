@article{ ISI:000361897200002,
Author = {Nita, Mihaela-Catalina and Pop, Florin and Voicu, Cristiana and Dobre,
   Ciprian and Xhafa, Fatos},
Title = {{MOMTH: multi-objective scheduling algorithm of many tasks in Hadoop}},
Journal = {{CLUSTER COMPUTING-THE JOURNAL OF NETWORKS SOFTWARE TOOLS AND
