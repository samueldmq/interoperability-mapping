@inproceedings{ ISI:000385259400014,
Author = {Domaschka, Joerg and Seybold, Daniel and Griesinger, Frank and Baur,
   Daniel},
Editor = {{Celesti, A and Leitner, P}},
Title = {{Axe: A Novel Approach for Generic, Flexible, and Comprehensive
   Monitoring and Adaptation of Cross-Cloud Applications}},
Booktitle = {{ADVANCES IN SERVICE-ORIENTED AND CLOUD COMPUTING (ESOCC 2015)}},
Series = {{Communications in Computer and Information Science}},
Year = {{2016}},
Volume = {{567}},
Pages = {{184-196}},
