@inproceedings{ ISI:000311860500002,
Author = {Abolfazli, Saeid and Sanaei, Zohreh and Shiraz, Muhammad and Gani,
   Abdullah},
Book-Group-Author = {{IEEE}},
Title = {{MOMCC: Market-Oriented Architecture for Mobile Cloud Computing Based on
   Service Oriented Architecture}},
